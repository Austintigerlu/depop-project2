<!DOCTYPE html>
<html lang="en">
  <%- include('../partials/head.ejs' ) %>

  <body>
    <%- include('../partials/nav.ejs') %>
    <section class="hero is-fullheight-with-navbar is-link">
        <div class="container">
            <div class="row">
                <div class = "col-sm-12 col-md-4 justify-content-end"></div>
                <div class="col-sm-6 col-md-4 col-md-offset-4 justify-content-end">
                    <h1 class="text-center login-title">Register</h1>
                    <div class="account-wall">
                        <img class="profile-img" src="data:image/jpeg;base64,">
                        <form action="/login/register" method="POST" class="form-signin">
                            <input name="username" type="username" class="form-control" placeholder="Username" required autofocus>
                            <input name="email" type="email" class="form-control" placeholder="Email" required autofocus>
                            <input name="password" type="password" class="form-control" placeholder="******" required>
                            <button class="btn btn-lg btn-primary btn-block" type="submit">Register</button>
                        </form>
                        <a href="/login" class="text-center new-account">Log In</a>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <main class="container is-max-widescreen"></main>
    <%- include('../partials/footer.ejs') %>
  </body>
</html>